void insert(stack<int>&s,int temp)            //working
{
    if(s.empty() || s.top()<=temp){
    s.push(temp);
    return;
    }
    int pp=s.top();
    s.pop();
    insert(s,temp);
    s.push(pp);
}


void SortedStack :: sort()
{
  if(s.size()==1)
  return;
  int temp=s.top();
  s.pop();
  sort();
  insert(s,temp);
  return;
}
